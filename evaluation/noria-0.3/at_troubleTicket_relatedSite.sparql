# GRLC metadata
#+ summary: Authoring Test for the NORIA-O data model
#+ method: GET
#+ tags:
#+   - AT | NORIA-O-0.3

PREFIX noria: <https://w3id.org/noria/ontology/>
PREFIX bot: <https://w3id.org/bot#>
SELECT ?tt ?res ?loc
WHERE {
	?tt a noria:TroubleTicket ;
		noria:troubleTicketRelatedResource ?res.
	?res rdf:type noria:Resource .
    ?loc bot:hasElement ?res
}