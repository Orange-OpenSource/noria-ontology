    <!-- https://w3id.org/noria/ontology/ChangeRequest -->


    <owl:Class rdf:about="https://w3id.org/noria/ontology/ChangeRequest">
        <rdfs:subClassOf rdf:resource="https://w3id.org/noria/ontology/DynamicElement"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/pep/ProcedureExecutionContainer"/>
