<?xml version="1.0"?>
<rdf:RDF xmlns="http://IBCNServices.github.io/Folio-Ontology/Folio.owl"
     xml:base="http://IBCNServices.github.io/Folio-Ontology/Folio.owl"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/">
    <owl:Ontology rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl">
        <owl:imports rdf:resource="http://www.w3.org/ns/ssn/"/>
        <terms:title xml:lang="en">Failure mode and effect analysis combined with anomaly ontology</terms:title>
        <foaf:name xml:lang="en">Folio</foaf:name>
        <terms:creator xml:lang="en">Bram Steenwinckel</terms:creator>
        <owl:versionInfo xml:lang="en">1.0.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2004/02/skos/core#definition -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#definition"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#example -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#example"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#happenedAt -->

    <owl:ObjectProperty rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#happenedAt">
        <rdfs:isDefinedBy>http://IBCNServices.github.io/Folio-Ontology/Folio.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">happenedAt</rdfs:label>
        <skos:definition xml:lang="en">Relation between the failure cause and the corresponding device/system.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasCategory -->

    <owl:ObjectProperty rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasCategory">
        <rdfs:isDefinedBy>http://IBCNServices.github.io/Folio-Ontology/Folio.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasCategory</rdfs:label>
        <skos:definition xml:lang="en">Relation between the observed element and its category</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasCause -->

    <owl:ObjectProperty rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasCause">
        <rdfs:isDefinedBy>http://IBCNServices.github.io/Folio-Ontology/Folio.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasCause</rdfs:label>
        <skos:definition xml:lang="en">Relation between a failure and it corresponding cause.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasCriticality -->

    <owl:ObjectProperty rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasCriticality">
        <rdfs:isDefinedBy>http://IBCNServices.github.io/Folio-Ontology/Folio.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasCriticality</rdfs:label>
        <skos:definition xml:lang="en">Relation between the observed element and the criticality</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasDetection -->

    <owl:ObjectProperty rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasDetection">
        <rdfs:isDefinedBy>http://IBCNServices.github.io/Folio-Ontology/Folio.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasDetection</rdfs:label>
        <skos:definition xml:lang="en">Relation between the observed element and the detection method</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasDomain -->

    <owl:ObjectProperty rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasDomain">
        <rdfs:isDefinedBy>http://IBCNServices.github.io/Folio-Ontology/Folio.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasDomain</rdfs:label>
        <skos:definition xml:lang="en">Relation between the observed element and its domain</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasEffect -->

    <owl:ObjectProperty rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasEffect">
        <rdfs:isDefinedBy>http://IBCNServices.github.io/Folio-Ontology/Folio.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasEffect</rdfs:label>
        <skos:definition xml:lang="en">Relation between a failure and it corresponding effect.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasFailureMode -->

    <owl:ObjectProperty rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasFailureMode">
        <rdfs:isDefinedBy>http://IBCNServices.github.io/Folio-Ontology/Folio.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasFailureMode</rdfs:label>
        <skos:definition xml:lang="en">Relation between the failure effect and the corresponding failure mode.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasLowerFailureEffect -->

    <owl:ObjectProperty rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasLowerFailureEffect">
        <rdfs:isDefinedBy>http://IBCNServices.github.io/Folio-Ontology/Folio.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasLowerFailureEffect</rdfs:label>
        <skos:definition xml:lang="en">Relation between a failure effect and a previously occurring effect.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasNextFailureEffect -->

    <owl:ObjectProperty rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasNextFailureEffect">
        <rdfs:isDefinedBy>http://IBCNServices.github.io/Folio-Ontology/Folio.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasNextFailureEffect</rdfs:label>
        <skos:definition xml:lang="en">Relation between a failure effect and a next effect.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasStatus -->

    <owl:ObjectProperty rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasStatus"/>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasSubAnomaly -->

    <owl:ObjectProperty rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasSubAnomaly"/>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#isEndEffect -->

    <owl:ObjectProperty rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#isEndEffect">
        <rdfs:isDefinedBy>http://IBCNServices.github.io/Folio-Ontology/Folio.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">isEndEffect</rdfs:label>
        <skos:definition xml:lang="en">Relation defining an failure effect as end effect.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#isIntermediateEffect -->

    <owl:ObjectProperty rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#isIntermediateEffect">
        <rdfs:isDefinedBy>http://IBCNServices.github.io/Folio-Ontology/Folio.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">isIntermediateEffect</rdfs:label>
        <skos:definition xml:lang="en">Relation defining an failure effect as intermediate effect.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#isLocalEffect -->

    <owl:ObjectProperty rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#isLocalEffect">
        <rdfs:isDefinedBy>http://IBCNServices.github.io/Folio-Ontology/Folio.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">isLocalEffect</rdfs:label>
        <skos:definition xml:lang="en">Relation defining an failure effect as the first mentioned effect.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#observedBy -->

    <owl:ObjectProperty rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#observedBy">
        <rdfs:isDefinedBy>http://IBCNServices.github.io/Folio-Ontology/Folio.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">observedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/sosa/hasResult -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/hasResult"/>
    


    <!-- http://www.w3.org/ns/sosa/usedProcedure -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/usedProcedure"/>
    


    <!-- http://www.w3.org/ns/ssn/wasOriginatedBy -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/wasOriginatedBy"/>
    


