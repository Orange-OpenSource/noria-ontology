    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#Anomaly -->

    <owl:Class rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#Anomaly">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/usedProcedure"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/ns/sosa/Procedure"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/ssn/wasOriginatedBy"/>
                <owl:someValuesFrom rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#fromObservation"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/ssn/wasOriginatedBy"/>
                <owl:someValuesFrom rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#observedProperty"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/ssn/wasOriginatedBy"/>
                <owl:someValuesFrom rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#toObservation"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/ssn/wasOriginatedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/ns/ssn/Stimulus"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/hasResult"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/ns/sosa/resultTime"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasCategory"/>
                        <owl:someValuesFrom rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#Category"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasCause"/>
                        <owl:someValuesFrom rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#Cause"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasCriticality"/>
                        <owl:someValuesFrom rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#Criticality"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasDetection"/>
                        <owl:someValuesFrom rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#DetectionMethod"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasDomain"/>
                        <owl:someValuesFrom rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#ConsequenceDomain"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasEffect"/>
                        <owl:someValuesFrom rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#Effect"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasSubAnomaly"/>
                <owl:someValuesFrom rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#Anomaly"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasStatus"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#Status"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:isDefinedBy>http://IBCNServices.github.io/Folio-Ontology/Folio.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Anomaly</rdfs:label>
        <skos:definition xml:lang="en">Description of possible unwanted behaviour</skos:definition>
    </owl:Class>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#Category -->

    <owl:Class rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#Category">
        <rdfs:isDefinedBy>http://IBCNServices.github.io/Folio-Ontology/Folio.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Category</rdfs:label>
        <skos:definition xml:lang="en">An ConfirmedAnomaly can happen once (transient), periodically (recurrent) or definitively (permanent).</skos:definition>
    </owl:Class>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#Cause -->

    <owl:Class rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#Cause">
        <rdfs:isDefinedBy>http://IBCNServices.github.io/Folio-Ontology/Folio.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Cause</rdfs:label>
        <skos:definition xml:lang="en">The cause refers to the description suspected to be the source of the ConfirmedAnomaly</skos:definition>
    </owl:Class>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#Confirmed -->

    <owl:Class rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#Confirmed">
        <rdfs:subClassOf rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#Status"/>
    </owl:Class>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#ConfirmedAnomaly -->

    <owl:Class rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#ConfirmedAnomaly">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasStatus"/>
                <owl:allValuesFrom rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#Confirmed"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#Anomaly"/>
        <rdfs:isDefinedBy>http://IBCNServices.github.io/Folio-Ontology/Folio.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">ConfirmedAnomaly</rdfs:label>
        <skos:definition xml:lang="en">ConfirmedAnomaly’s description. Should then express, at least, which part of the system is affected, by what. Ideally, this should be completed with additional information, such as which components observe the ConfirmedAnomaly, or the worth of its management.</skos:definition>
        <skos:example xml:lang="en">ConfirmedAnomaly’s description. Should then express, at least, which part of the system is affected, by what. Ideally, this should be completed with additional information, such as which components observe the ConfirmedAnomaly, or the worth of its management.</skos:example>
    </owl:Class>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#ConsequenceDomain -->

    <owl:Class rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#ConsequenceDomain">
        <rdfs:isDefinedBy>http://IBCNServices.github.io/Folio-Ontology/Folio.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">ConsequenceDomain</rdfs:label>
        <skos:definition xml:lang="en">Each domain relates to a specific kind of consequence of the ConfirmedAnomaly.</skos:definition>
        <skos:example xml:lang="en">Malfunctioning of device or dangerous situation for end user.</skos:example>
    </owl:Class>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#Criticality -->

    <owl:Class rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#Criticality">
        <rdfs:isDefinedBy>http://IBCNServices.github.io/Folio-Ontology/Folio.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Criticality</rdfs:label>
        <skos:definition xml:lang="en">It is a time dependent worth attributed to the ConfirmedAnomaly management.</skos:definition>
        <skos:example xml:lang="en">non-critic ConfirmedAnomaly</skos:example>
    </owl:Class>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#DetectionMethod -->

    <owl:Class rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#DetectionMethod">
        <rdfs:isDefinedBy>http://IBCNServices.github.io/Folio-Ontology/Folio.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">DetectionMethod</rdfs:label>
        <skos:definition xml:lang="en">This relies on components’ survey to trigger a warning about abnormalities.</skos:definition>
        <skos:example xml:lang="en">value &gt; x.</skos:example>
    </owl:Class>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#Effect -->

    <owl:Class rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#Effect">
        <rdfs:isDefinedBy>http://IBCNServices.github.io/Folio-Ontology/Folio.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Effect</rdfs:label>
        <skos:definition xml:lang="en">The effect describes one aspect of the observed phenomenon.</skos:definition>
    </owl:Class>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#FailureCause -->

    <owl:Class rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#FailureCause">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#isEndEffect"/>
                <owl:someValuesFrom rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#FailureMode"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#Cause"/>
        <rdfs:subClassOf rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#FailureMode"/>
        <rdfs:isDefinedBy>http://IBCNServices.github.io/Folio-Ontology/Folio.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">FailureCause</rdfs:label>
        <skos:definition xml:lang="en">Failure causes are defects in design, process, quality, or part application, which are the underlying cause of a failure or which initiate a process which leads to failure. Also, the failure effect at the highest indenture level or total system. A failure mode may have more causes.</skos:definition>
        <skos:example xml:lang="en">(abnormal) vibration input from another (possibly failed) system.</skos:example>
    </owl:Class>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#FailureEffect -->

    <owl:Class rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#FailureEffect">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasFailureMode"/>
