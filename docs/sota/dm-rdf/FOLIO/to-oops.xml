    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasEpochTime -->

    <owl:DatatypeProperty rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasEpochTime">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasLabeller -->

    <owl:DatatypeProperty rdf:about="http://IBCNServices.github.io/Folio-Ontology/Folio.owl#hasLabeller"/>
    


    <!-- http://www.w3.org/ns/sosa/resultTime -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/sosa/resultTime"/>
    


    <!-- 
