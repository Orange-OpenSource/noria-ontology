     xmlns:m3-lite="http://purl.org/iot/vocab/m3-lite#"
     xmlns:ssn="http://purl.oclc.org/NET/ssnx/ssn#"
     xmlns:geo="http://www.w3.org/2003/01/geo/wgs84_pos#"
