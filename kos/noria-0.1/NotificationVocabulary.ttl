<Notification/EventType>
  a skos:ConceptScheme ;
  skos:notation "eventType" ;
  skos:inScheme <Notification> ;
  skos:definition "The family of an event."@en ;
.

<Notification/EventTypeGroup>
  a skos:ConceptScheme ;
  skos:notation "eventTypeGroup" ;
  skos:inScheme <Notification> ;
  skos:definition "Group of EventType reflecting some specific alarming situation."@en ;
.

<Notification/EventTypeGroup/GeneralOperationAlarm>
  a skos:ConceptScheme ;
  skos:notation "eventTypeGroup-GeneralOperationAlarm" ;
  skos:inScheme <Notification> ;
  skos:definition "Group of EventType reflecting alarming situations about General Operation context issues."@en ;
  skos:example "The set of communicationAlarm, qualityofServiceAlarm, processingErrorAlarm, equipmentAlarm and environmentalAlarm event type."@en ;
.

<Notification/EventTypeGroup/SecurityAlarm>
  a skos:ConceptScheme ;
  skos:notation "eventTypeGroup-SecurityAlarm" ;
  skos:inScheme <Notification> ;
  skos:definition "Group of EvenType reflecting alarming situations about Security issues."@en ;
  skos:example "The set of integrityViolation, operationalViolation, physicalViolation, securityServiceOrMechanismViolation and timeDomainViolation event type."@en ;
.

<Notification/EventTypeGroup/ChangeOperation>
  a skos:ConceptScheme ;
  skos:notation "eventTypeGroup-ChangeOperation" ;
  skos:inScheme <Notification> ;
  skos:definition "Group of EvenType reflecting change operations on the ICT system within a ChangeRequest context."@en ;
.

<Notification/EventTypeGroup/CorrectiveMaintenanceOperation>
  a skos:ConceptScheme ;
  skos:notation "eventTypeGroup-CorrectiveMaintenanceOperation" ;
  skos:inScheme <Notification> ;
  skos:definition "Group of EvenType reflecting corrective maintenance operations on the ICT system within a TroubleTicket context."@en ;
.

