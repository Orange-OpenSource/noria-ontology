  skos:inScheme <Notification/EventType> ;
  skos:inScheme <Notification/EventTypeGroup/GeneralOperationAlarm> ;
  skos:notation "environmentalAlarm" ;
  skos:definition "This notification type is used to report a problem in the environment."@en ;
  skos:prefLabel "environmentalAlarm"@en ;
