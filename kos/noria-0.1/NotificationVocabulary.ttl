  skos:inScheme <Notification/EventTypeGroup/GeneralOperationAlarm> ;
  skos:notation "equipmentAlarm" ;
  skos:definition "This notification type is used to report a failure in the equipment."@en ;
  skos:prefLabel "equipmentAlarm"@en ;
  rdfs:isDefinedBy <https://www.itu.int/ITU-T/formal-language/itu-t/x/x721/1992/x721.html#-equipmentAlarmBehaviour> ;
.

<Notification/EventType/integrityViolation>
  a skos:Concept ;
  skos:inScheme <Notification/EventType> ;
  skos:inScheme <Notification/EventTypeGroup/SecurityAlarm> ;
  skos:notation "integrityViolation" ;
  skos:definition "This notification is used to report that a potential interruption in information flow has occurred such that information may have been illegally modified, inserted or deleted."@en ;
  skos:prefLabel "integrityViolation"@en ;
  rdfs:isDefinedBy <https://www.itu.int/ITU-T/formal-language/itu-t/x/x721/1992/x721.html#-integrityViolationBehaviour> ;
.

<Notification/EventType/objectCreation>
